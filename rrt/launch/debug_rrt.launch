<?xml version="1.0"?>
<launch>
	<include file="$(find rrt)/launch/parameters.launch" />
	<env name="ROSCONSOLE_CONFIG_FILE" value="$(find rrt)/custom_rosconsole.conf"/>
	<!-- Start the obstacle detection node -->
	<!-- <node
		pkg="pcl_converter"
		type="pcl_converter_node"
		name="pcl_converter_node"
		respawn="true"
		output="log"
	/> -->
	<!-- Start the RRT planner node -->
	<node
		pkg="rrt"
		type="rrt_node"
		name="rrt_node"
		output="screen"
		required="true"
		launch-prefix="xterm -e gdb -ex run --args"
	/>
	<!-- State estimator node -->
	<node
		pkg="state_estimator"
		type="state_estimator_node"
		name="state_estimator_node"
		respawn="true"
		output="log"
	/>
</launch>

